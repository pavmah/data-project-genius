sentiments_2017 <-
  bind_rows(
    overall_2017_anger,
    overall_2017_trust,
    overall_2017_anticipation,
    overall_2017_fear,
    overall_2017_joy,
    overall_2017_surprise,
    overall_2017_disgust,
    overall_2017_sadness
  )

library(ggplot2)

  ggplot(sentiments_2017,
         aes(
           x = date,
           y = total,
           color = sentiment,
           group = sentiment
         )) + 
  geom_line(size = 1) +
  geom_point(aes(key = artist), position = position_dodge(width = 0.8)) +
  
  xlab("Year") + 
  ylab("Number of sentiment words") + 
  ggtitle("Different sentiments in grime music over the last decade")  +
  
  theme_economist() 
  
  
  
  #2019 sentiments
  #anger
aj_19_anger <- aj_tracey %>% 
unnest_tokens(word, lyric) %>% 
anti_join(custom_stop_words_2) %>% 
semi_join(anger_sentiment) %>% 
count(word, sort = TRUE)

aj_19_anger$total = "56"
aj_19_anger$sentiment = "Anger"
aj_19_anger$artist = "AJ Tracey - self titled"
