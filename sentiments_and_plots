sentiments_2019 <-
  bind_rows(
    overall_2019_anger,
    overall_2019_trust,
    overall_2019_anticipation,
    overall_2019_fear,
    overall_2019_joy,
    overall_2019_surprise,
    overall_2019_disgust,
    overall_2019_sadness
  )


library(ggplot2)

albums_19 <- ggplot(sentiments_2019,
       aes(
         x = date,
         y = total,
         color = sentiment,
         group = sentiment
       )) + 
  geom_line(size = 1) +
  geom_point(aes(key = artist), position = position_dodge(width = 0.8)) +
  
  xlab("Year") + 
  ylab("Number of sentiment words") + 
  ggtitle("Different sentiments in grime music over the last decade")  +
  
  theme_economist() 


  
