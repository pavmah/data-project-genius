  bind_rows(
    overall_2016_anger,
    overall_2016_trust,
    overall_2016_anticipation,
    overall_2016_fear,
    overall_2016_joy,
    overall_2016_surprise,
    overall_2016_disgust,
    overall_2016_sadness
