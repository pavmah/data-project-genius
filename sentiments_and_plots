#plotting each artist/album's sentiment figures
#this ended up being done for only 4 years out of the 11 as it was too much for R to hold

#2009 anger
wiley_09_anger <- race_against_time %>%
  unnest_tokens(word, lyric) %>%
  anti_join(custom_stop_words_2) %>%
  semi_join(anger_sentiment) %>%
  count(word, sort = TRUE)

wiley_09_anger$total = "44"
wiley_09_anger$sentiment = "Anger"
wiley_09_anger$artist = "Wiley - Race Against Time"
wiley_09_anger$date = "08/06/09"
wiley_09_anger$date <- as.Date(wiley_09_anger$date, "%d/%m/%y")

###

tinchy_09_anger <- catch_22 %>%
  unnest_tokens(word, lyric) %>%
  anti_join(custom_stop_words_2) %>%
  semi_join(anger_sentiment) %>%
  count(word, sort = TRUE)

tinchy_09_anger$total = "51"
tinchy_09_anger$sentiment = "Anger"
tinchy_09_anger$artist = "Tinchy Stryder - Catch 22"
tinchy_09_anger$date = "17/08/09"
tinchy_09_anger$date <- as.Date(tinchy_09_anger$date, "%d/%m/%y")

###

skepta_09_anger <- microphone_champion %>%
  unnest_tokens(word, lyric) %>%
  anti_join(custom_stop_words_2) %>%
  semi_join(anger_sentiment) %>%
  count(word, sort = TRUE)

skepta_09_anger$total = "49"
skepta_09_anger$sentiment = "Anger"
skepta_09_anger$artist = "Skepta - Microphone Champion"
skepta_09_anger$date = "01/06/09"
skepta_09_anger$date <- as.Date(skepta_09_anger$date, "%d/%m/%y")

###

dizzee_09_anger <- tongue_n_cheek %>%
  unnest_tokens(word, lyric) %>%
  anti_join(custom_stop_words_2) %>%
  semi_join(anger_sentiment) %>%
  count(word, sort = TRUE)

dizzee_09_anger$total = "52"
dizzee_09_anger$sentiment = "Anger"
dizzee_09_anger$artist = "Dizzee Rascal - Tongue n Cheek"
dizzee_09_anger$date = "20/09/09"
dizzee_09_anger$date <- as.Date(dizzee_09_anger$date, "%d/%m/%y")

###

chipmunk_09_anger <- i_am_chipmunk %>%
  unnest_tokens(word, lyric) %>%
  anti_join(custom_stop_words_2) %>%
  semi_join(anger_sentiment) %>%
  count(word, sort = TRUE)

chipmunk_09_anger$total = "60"
chipmunk_09_anger$sentiment = "Anger"
chipmunk_09_anger$artist = "Chipmunk - I Am Chipmunk"
chipmunk_09_anger$date = "09/10/09"
chipmunk_09_anger$date <-
  as.Date(chipmunk_09_anger$date, "%d/%m/%y")

overall_2009_anger <-
  bind_rows(
    chipmunk_09_anger,
    dizzee_09_anger,
    skepta_09_anger,
    tinchy_09_anger,
    wiley_09_anger
  )

###

