library(ggplot2)

  ggplot(sentiments_2016,
         aes(
           x = date,
           y = total,
           color = sentiment,
           group = sentiment
         )) + 
  geom_line(size = 1) +
  geom_point(aes(key = artist), position = position_dodge(width = 0.8)) +
  
  xlab("Year") + 
  ylab("Number of sentiment words") + 
  ggtitle("Different sentiments in grime music over the last decade")  +
  
  theme_economist() 
  
  
  
  
  #anger
stormzy_17_anger <- gang_signs_and_prayer %>% 
unnest_tokens(word, lyric) %>% 
anti_join(custom_stop_words_2) %>% 
semi_join(anger_sentiment) %>% 
count(word, sort = TRUE)

stormzy_17_anger$total = "69"
stormzy_17_anger$sentiment = "Anger"
stormzy_17_anger$artist = "Stormzy - Gang Signs & Prayer"
stormzy_17_anger$date = "24/02/17"
stormzy_17_anger$date <- as.Date(stormzy_17_anger$date, "%d/%m/%y")

###

bugzy_17_anger <- king_of_the_north %>% 
unnest_tokens(word, lyric) %>% 
anti_join(custom_stop_words_2) %>% 
semi_join(anger_sentiment) %>% 
count(word, sort = TRUE)

bugzy_17_anger$total = "44"
bugzy_17_anger$sentiment = "Anger"
bugzy_17_anger$artist = "Bugzy Malone - King of the North"
bugzy_17_anger$date = "14/07/17"
bugzy_17_anger$date <- as.Date(bugzy_17_anger$date, "%d/%m/%y")

###

wiley_17_anger <- godfather %>%
unnest_tokens(word, lyric) %>% 
anti_join(custom_stop_words_2) %>% 
semi_join(anger_sentiment) %>% 
count(word, sort = TRUE)

wiley_17_anger$total = "63"
wiley_17_anger$sentiment = "Anger"
wiley_17_anger$artist = "Wiley - Godfather"
wiley_17_anger$date = "13/01/17"
wiley_17_anger$date <- as.Date(wiley_17_anger$date, "%d/%m/%y")

###

dizzee_17_anger <- raskit %>% 
unnest_tokens(word, lyric) %>% 
