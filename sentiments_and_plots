    overall_2009_joy,
    overall_2009_anger,
    overall_2009_anticipation,
    overall_2009_trust,
    overall_2009_disgust,
    overall_2009_sadness,
    overall_2009_surprise
  )

library(ggplot2)

  ggplot(sentiments_2009,
         aes(
           x = date,
           y = total,
           color = sentiment,
           group = sentiment
         )) + 
  geom_line(size = 1) +
  geom_point(aes(key = artist), position = position_dodge(width = 0.8)) +
  
  xlab("Year") + 
  ylab("Number of sentiment words") + 
  ggtitle("Different sentiments in grime music over the last decade")  +
  
  theme_economist() 



#anger
skepta_16_anger <- konnichiwa %>% 
unnest_tokens(word, lyric) %>% 
anti_join(custom_stop_words_2) %>% 
semi_join(anger_sentiment) %>% 
count(word, sort = TRUE)

skepta_16_anger$total = "52"
